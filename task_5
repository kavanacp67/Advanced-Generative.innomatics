
def analyze_post_engagement(likes_list):
    total_likes = 0

    # Calculate total likes using loop
    for like in likes_list:
        total_likes += like

    # Check viral condition
    if total_likes >= 1000:
        status = "Viral Post"
    else:
        status = "Normal Engagement"

    # Output
    print("Total Likes:", total_likes)
    print("Post Status:", status)

likes = [200, 300, 250, 300]

analyze_post_engagement(likes)
     




def check_medicine_stock(stock):

    # Check stock condition
    if stock < 10:
        status = "Low Stock Alert"
    else:
        status = "Stock Sufficient"

    # Output
    print("Medicine Stock:", stock)
    print("Status:", status)

check_medicine_stock(6)
     



def check_rainfall(rainfall_list, required_level):

    total = 0

    # Calculate total rainfall
    for rain in rainfall_list:
        total += rain

    # Calculate average
    average = total / len(rainfall_list)

    # Check adequacy
    if average >= required_level:
        status = "Adequate Rainfall"
    else:
        status = "Inadequate Rainfall"

    # Output
    print("Average Rainfall:", average)
    print("Rainfall Status:", status)

rainfall = [70, 75, 68, 80, 67]

check_rainfall(rainfall, 70)
     




def detect_duplicate_accounts(usernames):

    unique_usernames = set(usernames)

    # Check duplicates
    if len(usernames) != len(unique_usernames):
        result = "Yes"
    else:
        result = "No"

    # Output
    print("Duplicate Accounts Found:", result)

usernames = ["user1", "user2", "user3", "user1"]

detect_duplicate_accounts(usernames)
     




def check_eligibility(age):

    # Check eligibility
    if age >= 18:
        status = "Eligible"
    else:
        status = "Not Eligible"

    # Output
    print("Patient Age:", age)
    print("Eligibility Status:", status)

check_eligibility(21)
     




def filter_premium_crops(prices):

    premium_crops = []

    # Filter premium prices
    for price in prices:
        if price > 2000:
            premium_crops.append(price)

    # Output
    print("Premium Crops:", premium_crops)

prices = [1500, 2500, 1800, 3200, 1900]

filter_premium_crops(prices)
     





def check_system_health(errors):

    # Check system status
    if errors == 0:
        status = "Healthy"
    elif errors <= 5:
        status = "Minor Issues"
    else:
        status = "Critical Issues"

    # Output
    print("Error Count:", errors)
    print("System Status:", status)

check_system_health(7)
     




def check_transaction(amount):

    limit = 50000

    # Check transaction
    if amount <= limit:
        status = "Approved"
    else:
        status = "Rejected"

    # Output
    print("Transaction Amount:", amount)
    print("Transaction Status:", status)

check_transaction(60000)
     




def check_attendance(attendance_list):

    total = 0

    # Calculate total attendance
    for attendance in attendance_list:
        total += attendance

    percentage = total / len(attendance_list)

    # Check eligibility
    if percentage >= 75:
        status = "Eligible"
    else:
        status = "Not Eligible"

    # Output
    print("Attendance Percentage:", percentage)
    print("Exam Eligibility:", status)

attendance = [80, 75, 85, 70, 90]

check_attendance(attendance)
     





def calculate_bill(units):

    # Calculate bill using slabs
    if units <= 100:
        bill = units * 3

    elif units <= 200:
        bill = (100 * 3) + ((units - 100) * 5)

    else:
        bill = (100 * 3) + (100 * 5) + ((units - 200) * 7)

    # Classify usage
    if bill < 500:
        usage = "Low Usage"
    elif bill <= 1500:
        usage = "Moderate Usage"
    else:
        usage = "High Usage"

    # Output
    print("Units Consumed:", units)
    print("Total Bill:", bill)
    print("Usage Classification:", usage)

calculate_bill(250)
     
